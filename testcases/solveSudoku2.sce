// Hulpfunctie voor bovenstaande functie testSolveTB


function [Pjuist,Pleeg, Pfout] = percentageCorrIngevuld(lS, lsS, lO)
    totaalVakken = 0;
    Pjuist = 0;
    Pleeg = 0;
    Pfout = 0;
    for(t=1:length(lS))
        S = lS(t)
        sS = lsS(t)
        O = lO(t)
        for i=1:9
            for j=1:9
                if(S(i,j) == 0) then
                    totaalVakken = totaalVakken + 1                    
                    if sS(i,j) == O(i,j) then
                        Pjuist = Pjuist + 1
                    else if sS(i,j) == 0 then
                            Pleeg = Pleeg + 1
                        else
                           Pfout = Pfout + 1 
                        end  
                    end
                end
            end
        end
    end
    Pjuist = Pjuist / totaalVakken;
    Pleeg = Pleeg / totaalVakken;
    Pfout = Pfout / totaalVakken;
endfunction




